{
  "Telegram": {
    "BotToken": "YOUR_BOT_TOKEN_HERE",
    "AllowedUsers": []
  },
  "AiProvider": {
    "Provider": "DeepSeek"
  },
  "DeepSeek": {
    "ApiKey": "YOUR_DEEPSEEK_API_KEY_HERE",
    "Model": "deepseek-chat",
    "Temperature": 0.7,
    "MaxTokens": 2000,
    "ApiUrl": "https://api.deepseek.com/v1/chat/completions"
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY_HERE",
    "Model": "gpt-4o-mini",
    "Temperature": 0.7,
    "MaxTokens": 2000,
    "ApiUrl": "https://api.openai.com/v1/chat/completions"
  },
  "YandexGpt": {
    "OAuthToken": "YOUR_YANDEX_OAUTH_TOKEN_HERE",
    "FolderId": "YOUR_YANDEX_FOLDER_ID_HERE",
    "Model": "yandexgpt/latest",
    "Temperature": 0.6,
    "MaxTokens": 1000,
    "ApiUrl": "https://llm.api.cloud.yandex.net/foundationModels/v1/completion",
    "IamTokenUrl": "https://iam.api.cloud.yandex.net/iam/v1/tokens"
  },
  "YandexGeocoding": {
    "ApiKey": "YOUR_YANDEX_GEOCODING_API_KEY_HERE",
    "ApiUrl": "https://geocode-maps.yandex.ru/1.x/"
  },
  "ElevenLabs": {
    "ApiKey": "YOUR_ELEVENLABS_API_KEY_HERE",
    "VoiceId": "21m00Tcm4TlvDq8ikWAM",
    "ModelId": "eleven_multilingual_v2",
    "ApiUrl": "https://api.elevenlabs.io/v1/text-to-speech"
  },
  "Conversation": {
    "MaxHistoryMessages": 10,
    "SystemPrompt": "Ты полезный AI-ассистент в Telegram боте. Отвечай на вопросы пользователей кратко и по существу."
  },
  "ContentFilter": {
    "EnableFiltering": true,
    "BlockedWords": [
      "спам",
      "реклама"
    ],
    "WarnOnDetection": true
  },
  "Logging": {
    "ServiceName": "aiTelegramBot",
    "DeploymentEnvironment": "production",
    "GrafanaCloudAccessToken": "",
    "LokiEndpoint": "https://logs-prod-039.grafana.net/loki/api/v1/push",
    "LokiUserId": "",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
